{
  "manifest_version": 2,
  "name": "NOAuth",
  "description": "Identify OAuth flows and vulnerabilities",
  "version": "1.3.0",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "activeTab",
    "<all_urls>"
  ],
   "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{8b94c17c-463f-432b-a401-16a7f1763b21}"
    }
  },
  "icons": {
    "512": "icons/auth-512.png"
}
}

